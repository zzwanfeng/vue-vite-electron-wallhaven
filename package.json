{
  "name": "electron-desktop",
  "private": true,
  "version": "0.0.0",
  "main": "Electron/Main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "electron:dev": "electron .",
    "electron:serve": "concurrently -k \"npm run dev\" \"npm run electron\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "electron-edge-js": "^19.0.0",
    "element-plus": "^2.2.17",
    "install": "^0.13.0",
    "npm": "^8.19.2",
    "pinia": "^2.0.22",
    "vue": "^3.2.37",
    "wallpaper": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "electron": "^19.0.0",
    "electron-builder": "^23.3.3",
    "sass": "^1.55.0",
    "vite": "^3.1.0",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "zzwanfeng App",
    "copyright": "Copyright Â© 2021 kuari",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "./src/assets/logo.ico"
    },
    "win": {
      "icon": "./src/assets/logo.ico"
    },
    "linux": {
      "icon": "./src/assets/logo.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./src/assets/logo.ico",
      "uninstallerIcon": "./src/assets/logo.ico",
      "installerHeaderIcon": "./src/assets/logo.ico"
    },
    "files": [
      "dist/**/*",
      "Electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "env": {
    "node": true
  }
}
